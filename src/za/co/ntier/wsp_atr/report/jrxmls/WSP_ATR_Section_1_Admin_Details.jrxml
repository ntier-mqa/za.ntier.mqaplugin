    <!-- SQL -->
    <queryString>
        <![CDATA[
SELECT
    -- Header consolidation row
    COALESCE(v.orgname, '<Organisation Name>')                AS organisation_name,
    COALESCE(v.zz_sdl_no, '<SDL Number>')                     AS sdl_number,

    -- PLACEHOLDERS: replace once you confirm source
    '<SIC Code>'                                              AS sic_code,
    '<SIC Title>'                                             AS sic_title,

    -- 1.1 Organisation Details (mostly placeholders)
    COALESCE(v.orgname, '<Trading Name>')                     AS trading_name,
    '<Organisation Size>'                                     AS organisation_size,
    '<Subsector>'                                             AS subsector,
    '<Total Employees>'                                       AS total_employees,
    '<Employees Terminated>'                                  AS employees_terminated,
    '<Organisation Tel No>'                                   AS organisation_tel,
    '<Organisation Fax No>'                                   AS organisation_fax,
    '<Organisation Email>'                                    AS organisation_email,
    '<Organisation Physical Address>'                         AS organisation_physical_address,
    '<Physical Code>'                                         AS physical_code,
    '<Organisation Postal Address>'                           AS organisation_postal_address,
    '<Postal Code>'                                           AS postal_code,
    '<Province - Municipality>'                               AS province_municipality,
    '<Type of Organisation>'                                  AS type_of_organisation,
    '<Unionised>'                                             AS unionised,
    '<Core Business of Organisation>'                         AS core_business,

    -- 1.2 SDF Details (from view)
    COALESCE(TRIM(CONCAT(v.zzfirstname, ' ', v.zzmiddlename, ' ', v.zzsurname)), '<SDF Name>')
                                                            AS sdf_name,
    COALESCE(v.phone, '<Phone Number>')                      AS sdf_phone,
    COALESCE(v.phone2, '<Cell Number>')                      AS sdf_cell,
    COALESCE(v.email, '<Email Address>')                     AS sdf_email,
    COALESCE(v.zzsdfroletype, '<SDF Role>')                  AS sdf_role,

    -- 1.3 Banking Details (placeholders; wire to real source later)
    '<Bank Name>'                                             AS bank_name,
    '<Account Number>'                                        AS account_number,
    '<Account Holder>'                                        AS account_holder,
    '<Type of Account>'                                       AS account_type,
    '<Branch Name>'                                           AS branch_name,
    '<Branch Code>'                                           AS branch_code

FROM adempiere.zz_wsp_atr_report r
LEFT JOIN adempiere.zz_wsp_atr_submitted s
       ON s.zz_wsp_atr_submitted_id = r.zz_wsp_atr_submitted_id
LEFT JOIN adempiere.zzsdforganisation_v v
       ON v.zzsdforganisation_id = s.zzsdforganisation_id
WHERE r.zz_wsp_atr_report_id = $P{ZZ_WSP_ATR_Report_ID}
]]>
    </queryString>

